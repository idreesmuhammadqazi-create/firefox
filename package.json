{
  "name": "pseudorun-firefox-extension",
  "version": "1.0.0",
  "description": "Firefox extension for PseudoRun offline pseudocode editor",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-extension": "npm run build && zip -r pseudorun-extension.zip dist/ manifest.json background.js icons/ index.html",
    "preview": "vite preview"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.19.0",
    "@codemirror/commands": "^6.3.0",
    "@codemirror/language": "^6.10.0",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.24.0",
    "@lezer/highlight": "^1.2.0",
    "docx": "^9.5.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "mammoth": "^1.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}